class Dog(object):
    def __init__(self,name):
        self.name=name
    def getName(self):
        print "my dog name is ",self.name
    def talk(self):
        print "{0}'s barking sound is mung mung".format(self.name)
class Sichu(object):
    def __init__(self,name):
        self.name=name
    def getName(self):
        print "my dog name is ",self.name
    def talk(self):
        print "{0}'s barking sound is si si".format(self.name)
class Maltese(object):
    def __init__(self,name):
        self.name=name
    def getName(self):
        print "my dog name is ",self.name
    def talk(self):
        print "{0}'s barking sound is mal mal".format(self.name)
myDog1=Dog('puppy')

myDog2=Sichu('sichu')

myDog3=Maltese('maltese')

def lab14():
    myDog1.getName()    
    myDog2.getName()
    myDog3.getName()
    myDog1.talk()
    myDog2.talk()
    myDog3.talk()
def main():
    lab14()
if __name__=="__main__":
    main()
input()